{
  "name": "gajiandulu-api",
  "description": "GajianDulu Express API",
  "version": "1.0.0",
  "homepage": "https://gajiandulu.id",
  "main": "src",
  "keywords": [
    "gajiandulu"
  ],
  "author": {
    "name": "Refactory"
  },
  "contributors": [
    "Refactory.id"
  ],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/"
  },
  "engines": {
    "node": ">= 9.11.2",
    "yarn": ">= 1.7.0"
  },
  "_moduleAliases": {
    "@helpers": "src/helpers",
    "@services": "src/services",
    "@models": "src/models"
  },
  "scripts": {
    "reset:all": "node_modules/.bin/sequelize db:drop && node_modules/.bin/sequelize db:create && node_modules/.bin/sequelize db:migrate",
    "test": "npm run eslint && npm run mocha",
    "eslint": "node_modules/.bin/eslint --fix src/. test/. --config .eslintrc.json",
    "start": "node_modules/.bin/nodemon --exec node src/",
    "mocha": "node_modules/.bin/mocha test/ --recursive --exit",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "apollo-upload-server": "^6.0.0-alpha.1",
    "axios": "^0.18.1",
    "bcrypt": "^3.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "compression": "^1.7.3",
    "config": "^1.30.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "exceljs": "^1.7.0",
    "express": "^4.16.3",
    "express-validator": "^6.2.0",
    "fb": "^2.0.0",
    "graphql": "^0.13.2",
    "helmet": "^3.21.2",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.2.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "module-alias": "^2.0.6",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.2",
    "node-cron": "^2.0.3",
    "node-pre-gyp": "^0.12.0",
    "node-xlsx": "^0.14.1",
    "nodemailer": "^4.6.7",
    "passport-custom": "^1.0.5",
    "pdfmake": "^0.1.53",
    "querystring": "^0.2.0",
    "randomstring": "^1.1.5",
    "sequelize": "^4.44.3",
    "twilio": "^3.39.1",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.4",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.3",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "sequelize-cli": "^5.5.1"
  },
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/prettier --config --write",
      "npm run eslint",
      "git add"
    ]
  }
}
